<?xml version="1.0" encoding="UTF-8"?>
<web-app
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd"
         version="5.0">
    <servlet>
        <servlet-name>showQuestionServlet</servlet-name>
        <servlet-class>com.servlet.showQuestionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>showQuestionServlet</servlet-name>
        <url-pattern>/showQues</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>registerServlet</servlet-name>
        <servlet-class>com.servlet.registerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registerServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>com.servlet.loginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>showRecordServlet</servlet-name>
        <servlet-class>com.servlet.showRecordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>showRecordServlet</servlet-name>
        <url-pattern>/showRec</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>changeServlet</servlet-name>
        <servlet-class>com.servlet.changeServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>changeServlet</servlet-name>
        <url-pattern>/changeInfo</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>adminAuthorityServlet</servlet-name>
        <servlet-class>com.servlet.adminAuthorityServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adminAuthorityServlet</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>changeQuestionServlet</servlet-name>
        <servlet-class>com.servlet.changeQuestionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>changeQuestionServlet</servlet-name>
        <url-pattern>/changeQues</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>menu.jsp</welcome-file>
    </welcome-file-list>
</web-app>